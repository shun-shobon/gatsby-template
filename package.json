{
  "name": "gatsby-template",
  "version": "0.1.0",
  "description": "GatsbyJSの自分用テンプレートです。",
  "author": "shun-shobon <shun.shobon@gmail.com>",
  "license": "MIT",
  "private": true,
  "repository": "https://github.com/shun-shobon/gatsby-template.git",
  "scripts": {
    "build": "gatsby build",
    "serve": "gatsby serve",
    "dev": "gatsby develop",
    "check": "tsc",
    "eslint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "eslint:fix": "eslint 'src/**/*.{js,jsx,ts,tsx}' --fix",
    "stylelint": "stylelint 'src/**/*.{css,scss}'",
    "stylelint:fix": "stylelint 'src/**/*.{css,scss}' --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --fix",
    "*.{css,scss}": "stylelint --fix"
  },
  "dependencies": {
    "gatsby": "^2.31.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0"
  },
  "devDependencies": {
    "@types/node": "^14.14.2",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "eslint-config-prettier": "^6.14.0",
    "eslint-plugin-prettier": "^3.1.4",
    "gatsby-plugin-sass": "^2.3.17",
    "gatsby-plugin-typescript": "^2.4.22",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.2",
    "sass": "^1.27.0",
    "stylelint": "^13.7.2",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recess-order": "^2.2.0",
    "stylelint-config-standard-scss": "^1.1.0",
    "stylelint-order": "^4.1.0",
    "stylelint-prettier": "^1.1.2",
    "stylelint-scss": "^3.18.0",
    "typescript": "^4.0.3"
  }
}
